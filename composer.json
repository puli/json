{
    "name": "webmozart/json",
    "description": "A robust JSON decoder/encoder with support for schema validation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3.3|^7.0",
        "justinrainbow/json-schema": "^2.0",
        "seld/jsonlint": "^1.0",
        "webmozart/assert": "^1.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "sebastian/version": "^1.0.1",
        "symfony/filesystem": "^2.5"
    },
    "require-dev": {
      "symfony/serializer": "~2.2|~3.0.0"
    },
    "suggest": {
      "symfony/serializer": "To integrate with the Symfony Serializer."
    },
    "autoload": {
        "psr-4": {
            "Webmozart\\Json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmozart\\Json\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
